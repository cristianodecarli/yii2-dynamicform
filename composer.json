    "keywords": [
        "cristianodecarli",
        "yii2",
        "extension",
        "widget",
        "yii2-dynamicform",
        "copy DOM element",
        "yii2 dynamic form"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/cristianodecarli/yii2-dynamicform/issues",
        "wiki": "https://github.com/cristianodecarli/yii2-dynamicform/wiki/",
        "source": "https://github.com/cristianodecarli/yii2-dynamicform"
    },
    "authors": [
        {
            "name": "Cristiano de Carli",
            "email": "cristianodecarli@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13",
        "symfony/css-selector": "~2.8|~3.0|~4.0",
        "symfony/dom-crawler": "~2.8|~3.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "newerton\\dynamicform\\": "src"
        }
    }
}